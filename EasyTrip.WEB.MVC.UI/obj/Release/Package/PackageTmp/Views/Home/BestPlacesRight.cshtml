@model List<Blog>
<div class="box1-left">
    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.Images[0].PhotoUrl" style="width:120px; height:65px" alt="" />
        </div>
        <div class="bot-desc1">
            <p>@item.Content.Substring(0,150)...</p>
        </div>
        <div class="clear"></div>
    }
</div>
