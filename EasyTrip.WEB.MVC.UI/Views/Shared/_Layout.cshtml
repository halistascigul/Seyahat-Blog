<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Seyahat Blog</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="~/web/css/style.css" type="text/css" media="all" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href="~/web/css/slider.css" rel="stylesheet" type="text/css" media="all" />
    <link rel="stylesheet" href="~/web/css/style.css" type="text/css" media="all" />
    <link href="~/web/css/slider.css" rel="stylesheet" type="text/css" media="all" />
    <link href="/SingleWeb/css/bootstrap.css" rel='stylesheet' type='text/css' />
    <link href="/SingleWeb/css/style.css" rel='stylesheet' type='text/css' />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @{ Html.RenderAction("RenderHeader", "Home");}
    <div class="container body-content">
        @RenderBody()
        <hr />
        @{ Html.RenderAction("RenderFooter", "Home");}
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("Slide", false)
    @RenderSection("scripts", required: false)
    <script src="~/web/js/jquery.min.js"></script>
    <script src="~/web/js/responsiveslides.min.js"></script>
</body>
</html>
